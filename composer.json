{
    "name": "bibliopolis/event-notification-php-sdk",
    "description": "A PHP SDK for processing eBay event notifications. Bibliopolis aims to add new schemas for additional notiifications endpoints and fix problems with original repository.",
    "keywords": ["eBay", "Event Notification", "SDK", "Endpoint Validation", "bibliopolis"],
    "license": "ISC",
    "version": "1.0.2",
    "autoload": {
        "psr-4": {
            "EventNotificationPHPSdk\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Shuoyang Yao"
        },
        {
	    "name": "Matthew Baer"
        }
    ],
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.4",
        "lrucache/lrucache": "master-dev",
        "guzzlehttp/guzzle": "^7.4.3",
        "spatie/async": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "start": "php -S localhost:9000 src/examples/example.php",
        "test": "./vendor/bin/phpunit src/test/test.php"
    }
}
